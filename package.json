    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.0",
    "@biomejs/biome": "1.8.3",
    "@sentry/astro": "7.119.0",
    "@spotlightjs/astro": "1.0.20",
    "@types/bun": "1.1.8",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "lefthook": "1.7.15",
